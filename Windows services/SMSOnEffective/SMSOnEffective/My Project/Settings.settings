    </Setting>
    <Setting Name="GatewayPassword" Type="System.String" Scope="User">
      <Value Profile="(Default)">Gateway Password</Value>
    </Setting>
    <Setting Name="SMSToHOFOnly" Type="System.Boolean" Scope="User">
      <Value Profile="(Default)">True</Value>
    </Setting>
